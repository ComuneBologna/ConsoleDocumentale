			<xsd:element name="storicoVersioni" type="StoricoVersioniDocumento" minOccurs="0" maxOccurs="unbounded" />
			<xsd:element name="campiEditabili" type="CampoEditabile" minOccurs="0" maxOccurs="unbounded" />

			<xsd:element name="dataRicezione" type="xsd:dateTime" minOccurs="0" maxOccurs="1"></xsd:element>
			<xsd:element name="infoFormularioRifiutiDichiarazioneImpresaAmianto" type="InfoFormularioRifiutiDichiarazioneImpresaAmiantoSchema" minOccurs="0" maxOccurs="unbounded"></xsd:element>
			<xsd:element name="noteArchiviazione" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	
	<!-- definizione tipo TipologiaDocumentoEnumSchema -->
	<xsd:simpleType name="TipologiaDocumentoEnumSchema">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="COMUNICAZIONE_ALLA_PROCURA"></xsd:enumeration>
			<xsd:enumeration value="COMUNICAZIONE_CON_PROPRIETARI"></xsd:enumeration>
			<xsd:enumeration value="CONFERMA_COMPLETAMENTO_BONIFICA"></xsd:enumeration>
			<xsd:enumeration value="DOCUMENTI_ALTRI_SETTORI"></xsd:enumeration>
			<xsd:enumeration value="ORDINANZE_ALTRI_SETTORI"></xsd:enumeration>
			<xsd:enumeration value="PROPOSTA_DI_ORDINANZA"></xsd:enumeration>
			<xsd:enumeration value="VERBALE_SOPRALLUOGO"></xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- definizione dei dati del documento DatiFascicoloDocumentoAltroAmiantoSchema -->
	<xsd:complexType name="DatiFascicoloDocumentoAltroAmiantoSchema">
		<xsd:sequence>
			<xsd:element name="dataRicezioneInvio" type="xsd:dateTime" minOccurs="0" maxOccurs="1"></xsd:element>
			<xsd:element name="versoComunicazioneEnum" type="VersoComunicazioneEnumSchema" minOccurs="0" maxOccurs="1"></xsd:element>
			<xsd:element name="tipologiaDocumentoEnum" type="TipologiaDocumentoEnumSchema" minOccurs="0" maxOccurs="1"></xsd:element>
			<xsd:element name="noteArchiviazione" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	
	<!-- *** Fascicolo *** -->
	<xsd:complexType name="Fascicolo">
		<xsd:sequence>
			<xsd:element name="numeroFattura" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
			<xsd:element name="ragioneSociale" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
			<xsd:element name="pIva" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
			<xsd:element name="codicePIva" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
			<xsd:element name="protocollazione" minOccurs="0" maxOccurs="unbounded" type="ProtocollazioneCapofila"></xsd:element>
			<xsd:element name="procedimenti" minOccurs="1" maxOccurs="unbounded" type="procedimento"></xsd:element>
			<xsd:element name="operazioni" minOccurs="1" maxOccurs="1" type="operazioni"></xsd:element>
			<xsd:element name="operazioniSuperUtente" minOccurs="1" maxOccurs="1" type="operazioni"></xsd:element>
			<xsd:element name="collegamenti" minOccurs="1" maxOccurs="1" type="collegamenti"></xsd:element>
			<xsd:element name="praticheCollegate" minOccurs="1" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="praticaCollegata" type="praticaCollegata" minOccurs="0" maxOccurs="unbounded"></xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="note" type="xsd:string" minOccurs="0" maxOccurs="1" />
			<xsd:element name="EventiIter" minOccurs="0" maxOccurs="1" >
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element type="EventoIter" name="EventoIter" minOccurs="0" maxOccurs="unbounded"></xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="allegati" minOccurs="1" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element type="Documento" name="Documento" minOccurs="0" maxOccurs="unbounded"></xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="datiAmianto" minOccurs="0" maxOccurs="1">
				<xsd:complexType>
					<xsd:choice>
						<xsd:element name="datiFascicoloSitoAmianto" type="DatiFascicoloSitoAmianto" minOccurs="0" maxOccurs="1"></xsd:element>
						<xsd:element name="datiFascicoloSegnalazioneAmianto" type="DatiFascicoloSegnalazioneAmianto" minOccurs="0" maxOccurs="1"></xsd:element>
						<xsd:element name="datiFascicoloPraticaAmianto" type="DatiFascicoloPraticaAmianto" minOccurs="0" maxOccurs="1"></xsd:element>
						
						<xsd:element name="datiFascicoloDocumentoComunicazionePubblicoPrivatoAmiantoSchema" type="DatiFascicoloDocumentoComunicazionePubblicoPrivatoAmiantoSchema" minOccurs="0" maxOccurs="1"></xsd:element>
						<xsd:element name="datiFascicoloDocumentoVerificaAmiantoSchema" type="DatiFascicoloDocumentoVerificaAmiantoSchema" minOccurs="0" maxOccurs="1"></xsd:element>
						<xsd:element name="datiFascicoloDocumentoValutazioneAmiantoSchema" type="DatiFascicoloDocumentoValutazioneAmiantoSchema" minOccurs="0" maxOccurs="1"></xsd:element>
						<xsd:element name="datiFascicoloDocumentoImpegnoBonificaAmiantoSchema" type="DatiFascicoloDocumentoImpegnoBonificaAmiantoSchema" minOccurs="0" maxOccurs="1"></xsd:element>
						<xsd:element name="datiFascicoloDocumentoPianoManutenzioneControlloAmiantoSchema" type="DatiFascicoloDocumentoPianoManutenzioneControlloAmiantoSchema" minOccurs="0" maxOccurs="1"></xsd:element>
						<xsd:element name="datiFascicoloDocumentoOrdinanzaAmiantoSchema" type="DatiFascicoloDocumentoOrdinanzaAmiantoSchema" minOccurs="0" maxOccurs="1"></xsd:element>
						<xsd:element name="datiFascicoloDocumentoPianoLavoroAmiantoSchema" type="DatiFascicoloDocumentoPianoLavoroAmiantoSchema" minOccurs="0" maxOccurs="1"></xsd:element>
						<xsd:element name="datiFascicoloDocumentoFormularioRifiutiDichiarazioneImpresaAmiantoSchema" type="DatiFascicoloDocumentoFormularioRifiutiDichiarazioneImpresaAmiantoSchema" minOccurs="0" maxOccurs="1"></xsd:element>
						<xsd:element name="datiFascicoloDocumentoAltroAmiantoSchema" type="DatiFascicoloDocumentoAltroAmiantoSchema" minOccurs="0" maxOccurs="1"></xsd:element>
					</xsd:choice>
				</xsd:complexType>
			</xsd:element>
			
			<xsd:element name="datiAggiuntivi" minOccurs="0" maxOccurs="1">
			<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="valori" minOccurs="0" maxOccurs="1">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="valore" minOccurs="0" maxOccurs="unbounded">
										<xsd:complexType>
											<xsd:attribute name="nome" type="xsd:string" use="required" />
											<xsd:attribute name="valore" type="xsd:string" use="required" />
											<xsd:attribute name="descrizione" type="xsd:string" use="required" />
											<xsd:attribute name="tipo" type="xsd:string" use="required" />
											<xsd:attribute name="obbligatorio" type="xsd:boolean" use="required" />
											<xsd:attribute name="editabile" type="xsd:boolean" use="required" />
											<xsd:attribute name="visibile" type="xsd:boolean" use="required" />
											<xsd:attribute name="posizione" type="xsd:integer" use="required" />
										</xsd:complexType>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="modulistica">
		<xsd:sequence>
			<xsd:element name="valori" type="valoreModulo" minOccurs="0" maxOccurs="unbounded" />
			<xsd:element name="protocollazione" minOccurs="0" maxOccurs="unbounded" type="ProtocollazioneCapofila"></xsd:element>
			<xsd:element name="allegati" minOccurs="1" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element type="Documento" name="Documento" minOccurs="0" maxOccurs="unbounded"></xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="praticheCollegate" minOccurs="1" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="praticaCollegata" type="praticaCollegata" minOccurs="0" maxOccurs="unbounded"></xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="nome" type="xsd:string" />
	</xsd:complexType>
	<xsd:complexType name="valoreModulo">
				<xsd:sequence>
					<xsd:element name="tabella" minOccurs="0" maxOccurs="1">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="riga" minOccurs="0" maxOccurs="unbounded">
									<xsd:complexType>
										<xsd:sequence>
											<xsd:element name="colonne" type="valoreModulo" minOccurs="0"	maxOccurs="unbounded" />
										</xsd:sequence>
									</xsd:complexType>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="valoriSezione" minOccurs="0" maxOccurs="unbounded" type="valoreModulo" />
					
				</xsd:sequence>
				<xsd:attribute name="tipoNodo" type="tipoNodoModulistica" />
				<xsd:attribute name="tipo" type="xsd:string" />
				<xsd:attribute name="nome" type="xsd:string" />
				<xsd:attribute name="valore" type="xsd:string" />
				<xsd:attribute name="etichetta" type="xsd:string" />
				<xsd:attribute name="descrizione" type="xsd:string" />
				<xsd:attribute name="visibile" type="xsd:boolean" />
				<xsd:attribute name="titolo" type="xsd:string" />
	
	</xsd:complexType>
	<xsd:simpleType name="tipoNodoModulistica">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="SEZIONE" />
			<xsd:enumeration value="VALORE_MODULO" />
		</xsd:restriction>
	</xsd:simpleType>
	
	
	
	<xsd:complexType name="template">
		<xsd:sequence>
			<xsd:element name="nome" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
			<xsd:element name="descrizione" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
			
			<xsd:element name="oggettoMail" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
			
			<xsd:element name="corpoMail" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
			
			<xsd:element name="mittente" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
			
			<xsd:element name="destinatari" type="xsd:string" minOccurs="0" maxOccurs="unbounded"></xsd:element>
			
			<xsd:element name="destinatarioCC" type="xsd:string" minOccurs="0" maxOccurs="unbounded"></xsd:element>
			<xsd:element name="campo" type="campoTemplate" minOccurs="1" maxOccurs="unbounded" />
			<xsd:element name="fascicoliAbilitati" type="xsd:string" minOccurs="0" maxOccurs="unbounded"></xsd:element>
					
		</xsd:sequence>
	</xsd:complexType>


	<xsd:complexType name="campoTemplate">
		<xsd:sequence>
			<xsd:element name="nome" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
			<xsd:element name="formato" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
			<xsd:element name="tipo" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
			<xsd:element name="regexValidazione" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
			<xsd:element name="lunghezzaMassima" type="xsd:int" minOccurs="0" maxOccurs="1"></xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	
	
	<!-- definizione pratica di tipo PEC -->
	<xsd:complexType name="PEC">
		<xsd:sequence>
			<xsd:element name="messageID" minOccurs="1" maxOccurs="1" type="xsd:string"></xsd:element>
			<xsd:element name="dataRicezione" minOccurs="0" maxOccurs="1" type="xsd:dateTime"></xsd:element>
			<xsd:element name="dataInvio" minOccurs="0" maxOccurs="1" type="xsd:dateTime"></xsd:element>
			<xsd:element name="mittente" minOccurs="1" maxOccurs="1" type="xsd:string" />
			<xsd:element name="protocollazione" minOccurs="0" maxOccurs="1" type="ProtocollazionePEC"></xsd:element>
			<xsd:element name="interoperabile" minOccurs="0" maxOccurs="1" type="Interoperabile"></xsd:element>
			<xsd:element name="messageIDReinoltro" minOccurs="0" maxOccurs="1" type="xsd:string"></xsd:element>
			<xsd:element name="destinatari" minOccurs="1" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element type="destinatario" name="destinatarioPrincipale" minOccurs="1" maxOccurs="1" />
						<xsd:element type="destinatario" name="destinatario" minOccurs="0" maxOccurs="unbounded"></xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="destinatariCC" minOccurs="1" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element type="destinatario" name="destinatarioCC" minOccurs="0" maxOccurs="unbounded"></xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="destinatariInoltro" minOccurs="1" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element type="xsd:string" name="destinatarioInoltro" minOccurs="0" maxOccurs="unbounded"></xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="progressivoInoltro" minOccurs="0" maxOccurs="1" type="xsd:integer" />
			<xsd:element name="notificaRifiutoInoltro" minOccurs="1" maxOccurs="1" type="xsd:boolean" />
			<xsd:element name="tipoEmail" minOccurs="1" maxOccurs="1" type="xsd:string" />
			<xsd:element name="oggetto" minOccurs="0" maxOccurs="1" type="xsd:string" />
			<xsd:element name="body" minOccurs="0" maxOccurs="1" type="xsd:string" />
			<xsd:element name="note" type="xsd:string" minOccurs="0" maxOccurs="1" />
			<xsd:element name="allegati" minOccurs="1" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element type="Documento" name="Documento" minOccurs="0" maxOccurs="unbounded"></xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="hasRicevutaAccettazione" minOccurs="1" maxOccurs="1" type="xsd:boolean"></xsd:element>
			<xsd:element name="hasRicevutaConsegna" minOccurs="1" maxOccurs="1" type="xsd:boolean"></xsd:element>
			<xsd:element name="EventiIter" minOccurs="0" maxOccurs="1" >
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element type="EventoIter" name="EventoIter" minOccurs="0" maxOccurs="unbounded"></xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<!-- root element -->
	<xsd:element name="pratica">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="_version" minOccurs="1" maxOccurs="1" type="xsd:int" />
				<xsd:element name="_sync" minOccurs="1" maxOccurs="1" type="xsd:boolean" />
				<xsd:element name="idDocumentale" minOccurs="1" maxOccurs="1" type="xsd:string" />
				<xsd:element name="letto" minOccurs="1" maxOccurs="1" type="xsd:boolean" />
				<xsd:element name="folderPath" minOccurs="1" maxOccurs="1" type="xsd:string"></xsd:element>
				<xsd:element name="consoleFileName" minOccurs="1" maxOccurs="1" type="xsd:string"></xsd:element>
				<xsd:element name="provenienza" minOccurs="1" maxOccurs="1" type="xsd:string"></xsd:element>
				<xsd:element name="stato" minOccurs="1" maxOccurs="1" type="xsd:string"></xsd:element>
				<xsd:element name="utenteCreazione" minOccurs="0" maxOccurs="1" type="xsd:string"></xsd:element>
				<xsd:element name="usernameCreazione" minOccurs="0" maxOccurs="1" type="xsd:string"></xsd:element>
				<xsd:element name="dataCreazione" minOccurs="1" maxOccurs="1" type="xsd:dateTime"></xsd:element>
				<xsd:element name="tipo" minOccurs="1" maxOccurs="1" type="xsd:string"></xsd:element>
				<xsd:element name="titolo" minOccurs="0" maxOccurs="1" type="xsd:string"></xsd:element>
				<xsd:element name="fascicoloCollegato" minOccurs="0" maxOccurs="1" type="praticaCollegata"></xsd:element><!-- Usato da PEC , Modulistica e Fascicoli Amianto -->
				<xsd:element name="gruppivisibilita" type="gruppivisibilita" maxOccurs="1" minOccurs="1"/>
				<xsd:element name="condivisione" type="condivisione" maxOccurs="1" minOccurs="1"/>
				<xsd:element name="incaricoa" minOccurs="1" maxOccurs="1" type="utente" />
				<xsd:choice>
					<xsd:element type="PEC" name="PEC" minOccurs="0" maxOccurs="1"></xsd:element>
					<xsd:element type="Fascicolo" name="Fascicolo" minOccurs="0" maxOccurs="1"></xsd:element>
					<xsd:element type="modulistica" name="modulistica" minOccurs="0" maxOccurs="1"></xsd:element>
					<xsd:element type="template" name="template" minOccurs="0" maxOccurs="1"></xsd:element>
				</xsd:choice>
				<xsd:element name="tasks" minOccurs="0" maxOccurs="1">
					<xsd:complexType>
						<xsd:sequence><!-- refactor -->
							<xsd:element type="Task" name="Task" minOccurs="0" maxOccurs="unbounded" />
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
			<xsd:attribute name="versione" type="xsd:string" fixed="1.0" use="required" />
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
	<!-- *** AMIANTO *** -->
	<!-- *** Sezione: FASCICOLO AMIANTO *** -->
	<!-- definizione tipo StatoPraticaAmianto -->
	<xsd:simpleType name="StatoPraticaAmianto">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Aperta"></xsd:enumeration>
			<xsd:enumeration value="Chiusa"></xsd:enumeration>
			<xsd:enumeration value="Sospesa"></xsd:enumeration>
			<xsd:enumeration value="Eliminata"></xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- definizione tipo EsitoValutazioneAmianto -->
	<xsd:simpleType name="EsitoValutazioneAmianto">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Discreto"></xsd:enumeration>
			<xsd:enumeration value="Scadente"></xsd:enumeration>
			<xsd:enumeration value="Pessimo"></xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- riferimento all'alfresco path dei documenti amianto -->
	<xsd:complexType name="RiferimentoFascicoloDocumentoAmiantoSchema">
		<xsd:sequence>
			<xsd:element name="alfrescoPath" type="xsd:string"></xsd:element>
			<xsd:element name="titolo" type="xsd:string"></xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- definizione dati fascicolo pratica amianto -->
	<xsd:complexType name="DatiFascicoloPraticaAmianto">
		<xsd:sequence>
			<!-- campi obbligatori -->
			<xsd:element name="denominazione" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="codiceFiscale" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="indirizzo" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="titoloSoggetto" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="altroSoggetto" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="gradoAttendibilita" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			
			<xsd:element name="proprietariEdificioAmianto" type="ProprietariEdificioAmianto" minOccurs="0" maxOccurs="unbounded"></xsd:element>
			<xsd:element name="statoPratica" type="StatoPraticaAmianto" minOccurs="0" maxOccurs="1"></xsd:element>
			<xsd:element name="dataAperturaPratica" type="xsd:dateTime" minOccurs="0" maxOccurs="1"></xsd:element>
			<xsd:element name="dataChiusuraPratica" type="xsd:dateTime" minOccurs="0" maxOccurs="1"></xsd:element>

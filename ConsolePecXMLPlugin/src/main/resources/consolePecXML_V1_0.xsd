<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<!-- definizioni -->
	<xsd:complexType name="destinatario">
		<xsd:sequence>
			<xsd:element name="email" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
		</xsd:sequence>
		<xsd:attribute name="consegna" type="xsd:boolean"/>
		<xsd:attribute name="accettazione" type="xsd:boolean"/>
		<xsd:attribute name="tipo" type="xsd:string"/>
		<xsd:attribute name="errore" type="xsd:string"/>
	</xsd:complexType>
	<xsd:complexType name="gruppivisibilita">
		<xsd:sequence>
			<xsd:element name="gruppovisibilita" type="xsd:string" minOccurs="0" maxOccurs="unbounded"></xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="TipoProtocollazione">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="E" />
			<xsd:enumeration value="U" />
			<xsd:enumeration value="I" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="procedimento">
		<xsd:sequence>
			<xsd:element name="CodUtente" type="xsd:string" minOccurs="1"></xsd:element>
			<xsd:element name="PG" type="PG" minOccurs="1"></xsd:element>
			<xsd:element name="CodTipologiaProcedimento" type="xsd:integer" minOccurs="1"></xsd:element>
			<xsd:element name="DataInizioDecorrenzaProcedimento" type="xsd:dateTime" minOccurs="1"></xsd:element>
			<xsd:element name="ModAvvioProcedimento" type="xsd:string" minOccurs="1"></xsd:element>
			<xsd:element name="CodUnitaOrgResponsabile" type="xsd:integer" minOccurs="0"></xsd:element>
			<xsd:element name="CodUnitaOrgCompetenza" type="xsd:integer" minOccurs="0"></xsd:element>
			<xsd:element name="FlagInterruzione" type="xsd:string" minOccurs="0"></xsd:element>
			<xsd:element name="CodQuartiere" type="xsd:integer" minOccurs="0"></xsd:element>
			<xsd:element name="Durata" type="xsd:integer" minOccurs="1"></xsd:element>
			<xsd:element name="Termine" type="xsd:integer" minOccurs="1"></xsd:element>
			<xsd:element name="Responsabile" type="xsd:string" minOccurs="1"></xsd:element>
			<xsd:element name="Provenienza" type="xsd:string" minOccurs="1"></xsd:element>
			<xsd:element name="DataChiusura" type="xsd:dateTime" minOccurs="0"></xsd:element>
			<xsd:element name="ModalitaChiusura" type="xsd:string" minOccurs="0"></xsd:element>
			<xsd:element name="ChiusuraPG" type="PG" minOccurs="0"></xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PG">
		<xsd:sequence>
			<xsd:element name="numeroPG" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="annoPG" type="xsd:integer" minOccurs="1" maxOccurs="1"></xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ProtocollazionePEC">
		<xsd:sequence>
			<xsd:element name="numeroPG" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="annoPG" type="xsd:integer" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="numerofascicolo" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="annofascicolo" type="xsd:integer" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="titolo" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="rubrica" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="sezione" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="tipologiadocumento" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="oggetto" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="provenienza" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="utenteprotocollazione" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="dataprotocollazione" type="xsd:dateTime" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="numeroRegistro" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="annoRegistro" type="xsd:integer" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="capofila" type="PG" minOccurs="0" maxOccurs="1"></xsd:element>
			<xsd:element name="note" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
			<xsd:element name="tipoProtocollazione" type="TipoProtocollazione" minOccurs="1" maxOccurs="1"></xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Protocollazione">
		<xsd:sequence>
			<xsd:element name="numeroPG" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="annoPG" type="xsd:integer" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="numerofascicolo" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
			<xsd:element name="annofascicolo" type="xsd:integer" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="numeropraticamodulistica" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
			<xsd:element name="titolo" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="rubrica" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="sezione" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="tipologiadocumento" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="oggetto" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="provenienza" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="utenteprotocollazione" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="dataprotocollazione" type="xsd:dateTime" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="numeroRegistro" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="annoRegistro" type="xsd:integer" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="note" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
			<xsd:element name="tipoProtocollazione" type="TipoProtocollazione" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:choice>
				<xsd:element type="xsd:boolean" name="fromBA01" minOccurs="0" maxOccurs="1"/>
				<xsd:sequence>
					<xsd:element name="praticheCollegateProtocollate" minOccurs="0" maxOccurs="1">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="praticaCollegata" type="praticaCollegata" minOccurs="0" maxOccurs="unbounded"></xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="allegatiProtocollati" minOccurs="0" maxOccurs="1">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="allegatoProtocollato" type="Documento" minOccurs="0" maxOccurs="unbounded"></xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
			</xsd:choice>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ProtocollazioneCapofila">
		<xsd:sequence>
			<xsd:element name="numeroPG" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="annoPG" type="xsd:integer" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="numerofascicolo" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
			<xsd:element name="annofascicolo" type="xsd:integer" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="numeropraticamodulistica" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
			<xsd:element name="titolo" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="rubrica" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="sezione" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="tipologiadocumento" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="oggetto" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="provenienza" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="utenteprotocollazione" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="dataprotocollazione" type="xsd:dateTime" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="numeroRegistro" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="annoRegistro" type="xsd:integer" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="tipoProtocollazione" type="TipoProtocollazione" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="note" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
			<xsd:element name="protocollazioniCollegate" type="Protocollazione"	minOccurs="0" maxOccurs="unbounded" />
			<xsd:choice>
				<xsd:element type="xsd:boolean" name="fromBA01" minOccurs="0" maxOccurs="1" />
				<xsd:sequence>
					<xsd:element name="praticheCollegateProtocollate"
						minOccurs="1" maxOccurs="1">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="praticaCollegata" type="praticaCollegata" minOccurs="0" maxOccurs="unbounded"></xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="allegatiProtocollati" minOccurs="1" maxOccurs="1">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="allegatoProtocollato" type="Documento" minOccurs="0" maxOccurs="unbounded"></xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
			</xsd:choice>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Interoperabile">
		<xsd:sequence>
			<xsd:element name="stato" type="xsd:string" minOccurs="1" maxOccurs="1" /> <!-- valido, non valido, errore -->
			<xsd:element name="confermata" type="xsd:boolean" maxOccurs="1" />
			<xsd:element name="aggiornata" type="xsd:boolean"  maxOccurs="1" />
			<xsd:element name="annullata" type="xsd:boolean"  maxOccurs="1" />
 			<xsd:element name="oggettoMessaggio" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
 			<xsd:element name="provenienzaMessaggio" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>				
 			<xsd:element name="codiceAmministrazione" type="xsd:string" minOccurs="1" maxOccurs="1" />
			<xsd:element name="codiceAOO" type="xsd:string" minOccurs="1" maxOccurs="1" />
			<xsd:element name="codiceRegistro" type="xsd:string" minOccurs="1" maxOccurs="1" />
			<xsd:element name="numeroRegistrazione" type="xsd:string" minOccurs="1" maxOccurs="1" />
			<xsd:element name="dataRegistrazione" type="xsd:dateTime" minOccurs="1" maxOccurs="1" />
			<xsd:element name="nomeDocumento" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="oggettoDocumento" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="titoloDocumento" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="confermaRicezione" type="xsd:boolean"  maxOccurs="1" />
			<xsd:element name="tipologiaSegnatura" type="xsd:string"  maxOccurs="1" />
			<xsd:element name="nomeAllegatoPrincipale" type="xsd:string"  maxOccurs="1" />
			
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Documento">
		<xsd:sequence>
			<xsd:element name="nome" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="folderRelativePath" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="label" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="currentVersion" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
			<xsd:element name="dimensioneBytes" type="xsd:long" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="firmato" type="xsd:boolean" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="firmatoHash" type="xsd:boolean" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="tipoFirma" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="dataInizioPubblicazione" type="xsd:dateTime"></xsd:element>
			<xsd:element name="dataFinePubblicazione" type="xsd:dateTime"></xsd:element>
			<xsd:element name="gruppivisibilita" type="gruppivisibilita" maxOccurs="1" minOccurs="1"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="AssegnatarioTask">
		<xsd:sequence>
			<xsd:element name="nome" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="dataInizio" type="xsd:dateTime"></xsd:element>
			<xsd:element name="dataFine" type="xsd:dateTime"></xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="praticaCollegata">
		<xsd:sequence>
			<xsd:element name="alfrescoPath" minOccurs="1" maxOccurs="1" type="xsd:string"></xsd:element>
			<xsd:element name="tipo" minOccurs="1" maxOccurs="1" type="xsd:string">
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="EventoIter">
		<xsd:sequence>
		    <xsd:element name="annoPg" minOccurs="0" maxOccurs="1" type="xsd:int"></xsd:element>
		    <xsd:element name="numeroPg" minOccurs="0" maxOccurs="1" type="xsd:string"></xsd:element>
		    <xsd:element name="serialized" minOccurs="1" maxOccurs="1" type="xsd:boolean"></xsd:element>
		    <xsd:element name="isSerializationEnabled" minOccurs="1" maxOccurs="1" type="xsd:boolean"></xsd:element>
		    <xsd:element name="tipoEvento" minOccurs="1" maxOccurs="1" type="xsd:string"></xsd:element>
			<xsd:element name="testoEvento" minOccurs="1" maxOccurs="1" type="xsd:string"></xsd:element>
			<xsd:element name="currentUser" minOccurs="1" maxOccurs="1" type="xsd:string"></xsd:element>
			<xsd:element name="dataEvento" minOccurs="1" maxOccurs="1" type="xsd:dateTime"></xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="utente">
		<xsd:sequence>
			<xsd:element name="username" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="nome" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
			<xsd:element name="cognome" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
			<xsd:element name="matricola" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
			<xsd:element name="codicefiscale" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
			<xsd:element name="datapresaincarico" type="xsd:dateTime" minOccurs="0" maxOccurs="1"></xsd:element>
		</xsd:sequence>
		<xsd:attribute name="consegnato" type="xsd:boolean"/>
	</xsd:complexType>
	<!-- Sezione per dati di iper fascicolo -->
	<xsd:complexType name="condivisione">
		<xsd:sequence>
			<xsd:element name="nomeGruppo" type="xsd:string"  minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="dataInizio" type="xsd:dateTime"  minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="operazioni" type="operazioni" minOccurs="1" maxOccurs="1"></xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="elencoCondivisioni">
	<xsd:sequence>
		<xsd:element name="condivisioni" type="condivisione" minOccurs="1" maxOccurs="unbounded"></xsd:element>
	</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="operazione">
		<xsd:sequence>
			<xsd:element name="nomeOperazione" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="abilitata" type="xsd:boolean" minOccurs="1" maxOccurs="1"></xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="operazioni">
		<xsd:sequence>
			<xsd:element minOccurs="0" maxOccurs="unbounded" type="operazione" name="listaOperazioni"> </xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="collegamento">
		<xsd:sequence>
			<xsd:element name="gruppoCondivisione" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="dataCollegamento" type="xsd:dateTime" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="pathPratica" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
			<xsd:element name="operazioni" type="operazioni" minOccurs="1" maxOccurs="1"></xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="collegamenti">
		<xsd:sequence>
			<xsd:element name="elencoCollegamenti" type="collegamento" minOccurs="0" maxOccurs="unbounded"></xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- definizioni dei task -->
	<!-- definizione task generico -->
	<xsd:complexType name="Task">
		<xsd:sequence>
			<xsd:element name="id" type="xsd:integer" minOccurs="1" maxOccurs="1" /> <!-- obbligatorio per ogni task -->
			<xsd:element name="attivo" type="xsd:boolean" minOccurs="1" maxOccurs="1" default="true" />
			<xsd:element name="stato" minOccurs="1" maxOccurs="1" type="xsd:string" />
			<xsd:sequence>
				<xsd:element name="assegnatari" minOccurs="1" maxOccurs="1">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="assegnatarioCorrente" type="AssegnatarioTask" minOccurs="1" maxOccurs="1"></xsd:element>
							<xsd:element name="assegnatarioPassato" type="AssegnatarioTask" minOccurs="0" maxOccurs="unbounded"></xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
			<xsd:element name="condivisioni" type="elencoCondivisioni" minOccurs="0" maxOccurs="1"></xsd:element>
			<xsd:choice>
				<xsd:element type="gestionePEC" name="gestionePEC" minOccurs="0" maxOccurs="1"></xsd:element>
				<xsd:element type="gestioneFascicolo" name="gestioneFascicolo" minOccurs="0" maxOccurs="1"></xsd:element>
				<xsd:element type="gestioneModulistica" name="gestioneModulistica" minOccurs="0" maxOccurs="1"></xsd:element>
				<xsd:element type="gestioneTemplate" name="gestioneTemplate" minOccurs="0" maxOccurs="1"></xsd:element>
				<xsd:element type="riportaInGestione" name="riportaInGestione" minOccurs="0" maxOccurs="1"></xsd:element>
			</xsd:choice>
		</xsd:sequence>
	</xsd:complexType>
	<!-- definizione task di tipo riportaInGestione -->
	<xsd:complexType name="riportaInGestione">
		<xsd:sequence>
			<xsd:element name="idTaskDaRiattivare" type="xsd:integer" minOccurs="1" maxOccurs="1" />
		</xsd:sequence>
	</xsd:complexType>
	<!-- definizione task di tipo gestionePEC -->
	<xsd:complexType name="gestionePEC">

	</xsd:complexType>

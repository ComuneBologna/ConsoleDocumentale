<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:engit="urn:import:it.eng.portlet.consolepec.gwt.client.widget"
	xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui'>


	<g:HTMLPanel stylePrimaryName="filter">
		
		<g:HTMLPanel styleName="cell" ui:field="utentiPanel">
			<span class="label">Utente</span>
			<g:HTMLPanel styleName="abstract">
				<g:SuggestBox ui:field="utentiSuggestBox" styleName="testo"></g:SuggestBox>
			</g:HTMLPanel>
		</g:HTMLPanel>
		
		<g:HTMLPanel styleName="cell" ui:field="settoriPanel">
			<span class="label">Settore</span>
			<g:HTMLPanel styleName="abstract">
				<g:SuggestBox ui:field="settoriSuggestBox" styleName="testo"></g:SuggestBox>
			</g:HTMLPanel>
		</g:HTMLPanel>

		<g:HTMLPanel styleName="cell" ui:field="ruoliPanel">
			<span class="label">Gruppo*</span>
			<g:HTMLPanel styleName="abstract">
				<g:SuggestBox ui:field="gruppiSuggestBox" styleName="testo"></g:SuggestBox>
			</g:HTMLPanel>
		</g:HTMLPanel>
		
		<g:HTMLPanel styleName="cell" ui:field="operatoriPanel">
			<span class="label">Operatori</span>
			<g:HTMLPanel styleName="abstract">
				<g:SuggestBox ui:field="operatoriSuggestBox" styleName="testo"></g:SuggestBox>
			</g:HTMLPanel>
		</g:HTMLPanel>
		
	</g:HTMLPanel>

</ui:UiBinder>
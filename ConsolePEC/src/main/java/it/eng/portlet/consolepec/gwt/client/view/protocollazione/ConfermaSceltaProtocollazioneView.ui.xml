<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:engit="urn:import:it.eng.portlet.consolepec.gwt.client.widget"
	xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:HTMLPanel>
		<g:HTMLPanel>
			<h1 class="maintitle">
				<g:Label ui:field="titolo">
				</g:Label>
			</h1>
		</g:HTMLPanel>

		<g:HTMLPanel>
			<engit:MessageAlertWidget ui:field="messaggioAlertWidget"></engit:MessageAlertWidget>
			<g:HTMLPanel stylePrimaryName="single">
				<g:HTMLPanel stylePrimaryName="filters">
					<div class="cell">
						Vuoi protocollare?
					</div>
				</g:HTMLPanel>
				<div>
					<g:HTMLPanel stylePrimaryName="filters">
						<fieldset>
							<hr />
							<div class="cell">
								<g:Button ui:field="annullaButton" text="Annulla" styleName="btn black" />
								<g:Button ui:field="indietroButton" text="Indietro" styleName="btn black" />
								<g:Button ui:field="noButton" text="No" styleName="btn black" />
								<g:Button ui:field="siButton" text="Si" styleName="btn" />
							</div>
						</fieldset>
					</g:HTMLPanel>
				</div>
			</g:HTMLPanel>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder>

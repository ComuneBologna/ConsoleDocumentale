<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder 
	xmlns:engit="urn:import:it.eng.portlet.consolepec.gwt.client.widget" 
	xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:HTMLPanel>
		<g:HTMLPanel>
			<h1 class="maintitle"> Importa allegati email </h1>
		</g:HTMLPanel>
		
		<g:HTMLPanel stylePrimaryName="single">
			<g:HTMLPanel styleName="filters" >
		
				<fieldset>
					<div>
						<engit:MessageAlertWidget ui:field="messageWidget" />
					</div>
				</fieldset>
			
			<g:HTMLPanel  ui:field="treeAllegatiPanel">
		 		<g:DisclosurePanel ui:field="disclosurePanelEmailAllegati">	</g:DisclosurePanel>
			</g:HTMLPanel>
			
			<hr/>
			
			<g:HTMLPanel styleName="cell ">
				<g:Button ui:field="importaSelezionatiTreeButton" styleName="btn" enabled="false">Conferma</g:Button> 
				<g:Button ui:field="annullaButton" styleName="btn black">Annulla</g:Button> 
			</g:HTMLPanel>
			
			</g:HTMLPanel>
		</g:HTMLPanel>


	</g:HTMLPanel>
</ui:UiBinder>

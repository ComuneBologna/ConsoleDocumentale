<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:engit="urn:import:it.eng.portlet.consolepec.gwt.client.widget" xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<g:HTMLPanel>
		<g:HTMLPanel>
			<h1 class="maintitle" ui:field="titolo"> Dettaglio allegato pratica procedi</h1>
		</g:HTMLPanel>
		<!-- pulsanti in alto -->
		<div class="filters button-filters">
			<fieldset>
				<div class="inline-block">
					<g:Button ui:field="indietroButton" text="Indietro" styleName="btn black" />
				</div>
			</fieldset>
		</div>
		<hr />
		<!-- pannello di dettaglio popolato da codice -->
		<g:HTMLPanel stylePrimaryName="single" ui:field="content" />
		<!-- pannello di firma popolato da codice -->
		<g:HTMLPanel stylePrimaryName="single" ui:field="fdPanel" visible="false" />
	</g:HTMLPanel>
</ui:UiBinder>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:engit="urn:import:it.eng.portlet.consolepec.gwt.client.widget"
	xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:HTMLPanel>
		<g:HTMLPanel>
			<h1 class="maintitle">Condividi con</h1>
		</g:HTMLPanel>

		<g:HTMLPanel stylePrimaryName="single">
			<g:HTMLPanel styleName="filters">

				<fieldset>
					<div>
						<engit:MessageAlertWidget ui:field="messageWidget" />
						<h6>Ricerca Gruppi:</h6>
						<g:HTMLPanel styleName="acapo">
							<span class="label">Gruppi:</span>
							<g:HTMLPanel styleName="abstract">
								<g:HTMLPanel ui:field="elencoGruppiSuggestBoxPanel"	styleName="abstract"></g:HTMLPanel>
								<g:Button ui:field="aggiungiButton" styleName="btn">Aggiungi</g:Button>
							</g:HTMLPanel>
						</g:HTMLPanel>
					</div>
				</fieldset>

				<g:HTMLPanel  styleName="acapo">
					<h6>Elenco gruppi e utenti </h6>
				</g:HTMLPanel>
				
				
				<g:HTMLPanel styleName="operazioni">
					<g:HTMLPanel ui:field="mainCollegaPanel" />
				</g:HTMLPanel>

				<div class="filters">
				<fieldset>
					<hr />
					<div class="cell">
						<g:Button ui:field="annullaButton" text="Annulla" styleName="btn black" />
						<g:Button ui:field="confermaButton" text="Conferma"	styleName="btn" />
					</div>
				</fieldset>
			</div>

			</g:HTMLPanel>
		</g:HTMLPanel>


	</g:HTMLPanel>
</ui:UiBinder>

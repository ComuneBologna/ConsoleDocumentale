<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:d="urn:import:it.eng.portlet.consolepec.gwt.client.drive.widget">
	<g:HTMLPanel stylePrimaryName="drive-file">
		<h3>Carica file</h3>
		<d:DriveMessageWidget ui:field="message" />
		<g:HTMLPanel stylePrimaryName="drive-file">
			<g:HTMLPanel stylePrimaryName="drive-metadati-title">
				<g:InlineLabel>Dati file</g:InlineLabel>
			</g:HTMLPanel>
			<g:HTMLPanel stylePrimaryName="drive-metadati-box">
				<g:HTMLPanel stylePrimaryName="metadato-box">
					<g:InlineLabel>Cartella padre</g:InlineLabel>
					<g:InlineLabel ui:field="cartellaPadreLabel" />
				</g:HTMLPanel>
				<g:HTMLPanel stylePrimaryName="metadato-box">
					<g:InlineLabel>Percorso alfresco</g:InlineLabel>
					<g:InlineLabel ui:field="pathAlfrescoLabel" />
				</g:HTMLPanel>
				<g:HTMLPanel stylePrimaryName="metadato-box">
					<g:InlineLabel>File</g:InlineLabel>
					<d:DriveFileUploadWidget ui:field="uploadWidget" />
				</g:HTMLPanel>
				<g:HTMLPanel stylePrimaryName="metadato-box" ui:field="ruoliBox">
					<g:InlineLabel>Gruppo</g:InlineLabel>
					<g:ListBox ui:field="listBoxRuoli" />
				</g:HTMLPanel>
				<g:HTMLPanel stylePrimaryName="metadato-box">
					<g:InlineLabel>Metadati</g:InlineLabel>
					<g:CheckBox ui:field="checkMetadati" />
				</g:HTMLPanel>
			</g:HTMLPanel>
		</g:HTMLPanel>
		<g:HTMLPanel ui:field="metadatiPanel" />
		<g:HTMLPanel ui:field="buttonPanel" stylePrimaryName="drive-fun-box" />
	</g:HTMLPanel>
</ui:UiBinder>
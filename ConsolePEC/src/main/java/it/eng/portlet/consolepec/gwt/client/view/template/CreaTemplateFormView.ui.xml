<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'
	xmlns:engit="urn:import:it.eng.portlet.consolepec.gwt.client.widget"
	xmlns:engittemplate="urn:import:it.eng.portlet.consolepec.gwt.client.widget.template"
	xmlns:dp="urn:import:com.google.gwt.user.datepicker.client">

	<g:HTMLPanel>
		<g:HTMLPanel>
			<h1 class="maintitle"> Crea nuovo modello </h1>
		</g:HTMLPanel>


		<g:HTMLPanel stylePrimaryName="single">
			<engit:MessageAlertWidget ui:field="messaggioAlertWidget"></engit:MessageAlertWidget>
			
			<g:HTMLPanel stylePrimaryName="filters">
				<fieldset>
					
					<g:HTMLPanel ui:field="sceltaTemplateTitle" visible="false">
						<h6 > Scelta modello</h6>
					</g:HTMLPanel>
				
					<g:HTMLPanel styleName="cell acapo" ui:field="sceltaTemplatePanel" visible="false">
						<span class="label">Tipologia modello</span>
						<g:HTMLPanel styleName="abstract">
							<g:ListBox ui:field="sceltaTemplateListBox" stylePrimaryName="selectListBox">
							</g:ListBox>
						</g:HTMLPanel>
					</g:HTMLPanel>
					
					<g:HTMLPanel styleName="cell acapo" ui:field="corpoTemplatePanel" visible="false">
						
					</g:HTMLPanel>
					
				</fieldset>
			</g:HTMLPanel>
			
			<g:HTMLPanel>
				<div class="filters">
					<fieldset>
						<div class="cell">
							<g:Button ui:field="annullaButton" text="Annulla" styleName="btn black" />
							<g:Button ui:field="avantiButton" text="Avanti" styleName="btn" />
						</div>
					</fieldset>
				</div>
			</g:HTMLPanel>
		</g:HTMLPanel>

	</g:HTMLPanel>
</ui:UiBinder>

<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:ConsoleDisclosurePanel ui:field="indirizziDisclosure" animationEnabled="true" open="true">
		<g:HTMLPanel>
			<g:HTMLPanel stylePrimaryName="filters">
				<fieldset>
					<div class="cell acapo">
						<span class="label">Tipologia</span>
						<g:HTMLPanel styleName="abstract">
							<g:TextBox ui:field="tipologiaBox" />
						</g:HTMLPanel>
					</div>

					<div class="cell acapo">
						<span class="label">Via</span>
						<g:HTMLPanel styleName="abstract">
							<g:TextBox ui:field="viaBox" />
						</g:HTMLPanel>
					</div>

					<div class="cell acapo">
						<span class="label">Civico</span>
						<g:HTMLPanel styleName="abstract">
							<g:TextBox ui:field="civicoBox" />
						</g:HTMLPanel>
					</div>
					<div class="cell acapo">
						<span class="label">Esponente</span>
						<g:HTMLPanel styleName="abstract">
							<g:TextBox ui:field="esponenteBox" />
						</g:HTMLPanel>
					</div>
					<div class="cell acapo">
						<span class="label">Interno</span>
						<g:HTMLPanel styleName="abstract">
							<g:TextBox ui:field="internoBox" />
						</g:HTMLPanel>
					</div>
					<div class="cell acapo">
						<span class="label">Piano</span>
						<g:HTMLPanel styleName="abstract">
							<g:TextBox ui:field="pianoBox" />
						</g:HTMLPanel>
					</div>
					
					<div class="cell acapo">
						<span class="label">Comune</span>
						<g:HTMLPanel styleName="abstract">
							<g:TextBox ui:field="comuneBox" />
						</g:HTMLPanel>
					</div>
					
					<div class="cell acapo">
						<span class="label">CAP</span>
						<g:HTMLPanel styleName="abstract">
							<g:TextBox ui:field="capBox" />
						</g:HTMLPanel>
					</div>
					
					<div class="cell acapo">
						<span class="label">Nazione</span>
						<g:HTMLPanel styleName="abstract">
							<g:TextBox ui:field="nazioneBox" />
						</g:HTMLPanel>
					</div>

					<div class="cell acapo">
						<g:Button ui:field="aggiungiIndirizzo" text="Aggiungi Indirizzo" styleName="btn" />
					</div>

				</fieldset>
			</g:HTMLPanel>

			<g:HTMLPanel ui:field="indirizziPanel" stylePrimaryName="filters" />

		</g:HTMLPanel>
	</g:ConsoleDisclosurePanel>
</ui:UiBinder>
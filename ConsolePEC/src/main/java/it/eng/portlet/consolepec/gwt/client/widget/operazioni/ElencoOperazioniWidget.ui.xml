<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" 
             xmlns:g="urn:import:com.google.gwt.user.client.ui" 
             xmlns:d="urn:import:com.google.gwt.dom.client" 
             xmlns:h="urn:import:com.gargoylesoftware.htmlunit.html">
	<g:HTMLPanel styleName="operazioni">
		<fieldset>
			<g:HTMLPanel styleName="headerOperazioni">
				<g:Image ui:field="iconaImage"></g:Image>
				<span class="documento">
					<g:InlineLabel ui:field="titoloLabel"></g:InlineLabel>
				</span>
				<g:Button ui:field="eliminaButton" styleName="btn black">Elimina</g:Button>
			</g:HTMLPanel>
			<g:DisclosurePanel ui:field="disclosurePanel">
				<g:HTMLPanel ui:field="elencoPanel" styleName="elencoOperazioni" />
			</g:DisclosurePanel>
		</fieldset>
	</g:HTMLPanel>
</ui:UiBinder> 
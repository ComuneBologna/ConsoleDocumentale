<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
   <g:HTMLPanel styleName="documenti-mail" ui:field="mainPanel">
		
		<div ui:field="documentoContainer" class="documento-container">
		
		<g:CheckBox styleName="checkbox-nonprot" ui:field="checkBox"></g:CheckBox>

		<span class="documento-semplice">
			<g:Image ui:field="iconaLabel" styleName="ico-mail" title="Fascicolo collegato"></g:Image>
			<g:Anchor ui:field="linkFascicoloCollegato"/>
		</span>
		</div>
		
		<div class="documento-azioni">
			<g:Button ui:field="mostraDettaglioFascicoloCollegato" text="Mostra dettagli" styleName="btn black"></g:Button>
		</div>
		
   </g:HTMLPanel>

</ui:UiBinder> 
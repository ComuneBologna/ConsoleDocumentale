<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:engit="urn:import:it.eng.portlet.consolepec.gwt.client.widget" xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateLocales='default'>

	<g:HTMLPanel>
		<g:HTMLPanel>
			<h1 class="maintitle"> Modifica Tag allegati </h1>
		</g:HTMLPanel>

		<div style="padding-left: 10px;">
			<g:HTMLPanel stylePrimaryName="single">
				<engit:MessageAlertWidget ui:field="messageAlertWidget"></engit:MessageAlertWidget>
				<g:HTMLPanel stylePrimaryName="filters">
					<h4>Allegati selezionati</h4>
					<g:HTMLPanel ui:field="allegatiSelezionatiPanel" />
					<h4>Tag impostabili</h4>
					<g:HTMLPanel ui:field="tipologiePanel" />
				</g:HTMLPanel>
				<g:HTMLPanel>
					<div class="filters">
						<fieldset>
							<hr />
							<div class="cell">
								<g:Button ui:field="annullaButton" text="Annulla" styleName="btn black" />
								<g:Button ui:field="confermaButton" text="Conferma" styleName="btn" />
							</div>
						</fieldset>
					</div>
				</g:HTMLPanel>
			</g:HTMLPanel>
		</div>

	</g:HTMLPanel>
</ui:UiBinder>

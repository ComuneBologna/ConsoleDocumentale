<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
			 xmlns:g='urn:import:com.google.gwt.user.client.ui' 
			 ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
			 ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
			 ui:generateLocales='default'>
	<g:HTMLPanel ui:field="panel" styleName="apploading-blackout">
		<g:HTMLPanel styleName="apploading-content modal bordiarrotondatibottom bordiarrotondatitop" ui:field="internalPanel">
			<g:HTMLPanel ui:field="contentPanel" />
				<g:HTMLPanel styleName="allineaAlCentro bordiarrotondatibotton" ui:field="terminaButtonPanel" visible="false">
					<div class="filters button-filters">
						<fieldset>
							<div class="inline-block">
								<g:Button text="Attendi" ui:field="attendiButton"	styleName="btn black"></g:Button>
							</div>
							<div class="inline-block">
								<g:Button text="Termina" ui:field="terminaButton" styleName="btn"></g:Button>
							</div>
						</fieldset>
					</div>
					<br></br>
				</g:HTMLPanel>
				<g:HTMLPanel styleName="allineaAlCentro bordiarrotondatibotton" ui:field="confermaButtonPanel" visible="false">
					<div class="filters button-filters">
						<fieldset>
							<div class="inline-block">
								<g:Button text="Annulla" ui:field="annullaButton"	styleName="btn black"></g:Button>
							</div>
							<div class="inline-block">
								<g:Button text="Conferma" ui:field="confermaButton" styleName="btn"></g:Button>
							</div>
						</fieldset>
					</div>
					<br></br>
				</g:HTMLPanel>
		        <g:Image ui:field="imageCaricamento" />
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder>

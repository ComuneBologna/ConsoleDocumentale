<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:engit="urn:import:it.eng.portlet.consolepec.gwt.client.widget"
	xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:HTMLPanel>

		<g:HTMLPanel>
			<h1 class="maintitle">Cambia tipo fascicolo</h1>
		</g:HTMLPanel>

		<g:HTMLPanel stylePrimaryName="single">
			<engit:MessageAlertWidget ui:field="messageAlertWidget"></engit:MessageAlertWidget>
			<g:HTMLPanel stylePrimaryName="filters">
				<fieldset>
					<h6>Selezionare il nuovo tipo di fascicolo:</h6>
					<g:HTMLPanel styleName="cell acapo">
						<span class="label">Tipo fascicolo</span>
						<g:HTMLPanel styleName="abstract">
							<g:ListBox ui:field="tipoFascicolo" stylePrimaryName="selectListBox" />
						</g:HTMLPanel>
					</g:HTMLPanel>
				</fieldset>
			</g:HTMLPanel>
		</g:HTMLPanel>

		<g:HTMLPanel>
			<div class="filters">
				<fieldset>
					<hr />
					<div class="cell">
						<g:Button ui:field="confermaButton" styleName="btn" text="Conferma" />
						<g:Button ui:field="annullaButton" styleName="btn black"
							text="Annulla" />
					</div>
				</fieldset>
			</div>
		</g:HTMLPanel>

	</g:HTMLPanel>
</ui:UiBinder>

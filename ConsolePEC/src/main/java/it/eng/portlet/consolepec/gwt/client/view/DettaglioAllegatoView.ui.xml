<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:engit="urn:import:it.eng.portlet.consolepec.gwt.client.widget" xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui' ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat' ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator' ui:generateLocales='default'>

	<g:HTMLPanel>

		<g:HTMLPanel>
			<h1 class="maintitle" ui:field="titolo"> Dettaglio allegato</h1>
		</g:HTMLPanel>
		
		<g:HTMLPanel>
			<div class="filters">
				<fieldset>
					<!-- Messaggio d'errore -->
					<engit:MessageAlertWidget ui:field="messageWidget" />
					
					<div class="cell">
						<g:Button ui:field="chiudiButton" text="Chiudi" styleName="btn black" />
					</div>
				</fieldset>
			</div>

		</g:HTMLPanel>
		<hr />
		<g:HTMLPanel stylePrimaryName="single" ui:field="contenutoPanel">
			<span class="label">P.G.</span>
			<g:HTMLPanel styleName="abstract">
				<g:Label ui:field="pgLabel"></g:Label>
			</g:HTMLPanel>
			<hr />
			<span class="label">P.G.</span>
			<g:HTMLPanel styleName="abstract">
				<g:Label ui:field="pgLabe2l"></g:Label>
			</g:HTMLPanel>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder>

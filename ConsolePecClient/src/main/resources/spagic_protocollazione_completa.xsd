    	<xs:sequence>
    		<xs:element name="reci1" type="reci1" maxOccurs="1" minOccurs="1"></xs:element>
    		<xs:element name="reci2" type="reci2" maxOccurs="25" minOccurs="0"></xs:element>
    		<xs:element name="reci3" type="reci3" maxOccurs="1" minOccurs="0"></xs:element>
    		<xs:element name="reci5" type="reci5" maxOccurs="100" minOccurs="1"></xs:element>

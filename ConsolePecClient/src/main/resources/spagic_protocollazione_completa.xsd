    			<xs:element name="reci7e" type="reci7e" maxOccurs="1" minOccurs="0"></xs:element>
    			<xs:element name="reci7u" type="reci7u" maxOccurs="1" minOccurs="0"></xs:element>
    		</xs:choice>
    		<xs:element name="reciz" type="reciz" maxOccurs="1" minOccurs="0"></xs:element>
    	</xs:sequence>
    </xs:complexType>
    

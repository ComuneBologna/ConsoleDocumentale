		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="reco1">
		<xs:sequence>
			<xs:element name="o1_codice_messaggio" type="xs:string"></xs:element>
			<xs:element name="o1_descrizione_messsaggio" type="xs:string"></xs:element>
